package org.conceptcomputing.pharacologicCalc.dosage;

import XXX;

/**
*/
public serial class StockSolution {
   Unique(StockID);
   public string StockID;
   public Drug drug;    
   public meas drugMass  {drugMass.dim = mass; drugMass.acc = .001 gm}
   public meas solnVol {solVol.dim = vol; solVol.acc = .0001 ml}
         // solvent is assumed to be isotonic saline
   public meas drugConcentration 
      {  drugConcentration = drugMass / solVol
      [] drugConcentration.dim = molarity
      }
   
   test 1+-.001 molar { // molarity = mg/l
   [] drug = NaCl
   [] drugMass = 1 µg
   [] solnVol = 1 ml
   [] result = drugConcentration
   }

} // end class 
